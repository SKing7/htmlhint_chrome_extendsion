{
    "manifest_version":2,
    "name":"HtmlHint_chrome_extension",
    "description":"检查html的正确性",
    "version":"1.0",
    "permissions":[
        "tabs",
        "http://*/*"
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
